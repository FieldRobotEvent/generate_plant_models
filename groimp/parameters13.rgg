import static parameters.*;

/* soy */

{
	// field parameters
	nrRows[soy] = 5;                  // number of rows
	nrPlants[soy] = 1;                // number of plants in a row
	rowDistance[soy] = 0.15;          // distance between rows
	plantDistance[soy] = 0.15;        // distance between plants in a row
