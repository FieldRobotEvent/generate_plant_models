	leafCurve[soy] = 30;              // leaf curvature - angle between bottom and top of leaf blade (0 = flat)

	rankLower[soy] = 1;				// final phtyomer that has properties of lower phytomers (e.g. nr of leaflets, leaf angle, etc) 

